{
  "extends": ["@tsconfig/strictest", "@tsconfig/esm", "@tsconfig/next"],
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "exactOptionalPropertyTypes": false,
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "Bundler",
  },
  "include": ["**/*.ts", "**/*.tsx",],
  "exclude": ["lib", "node_modules"],
}
