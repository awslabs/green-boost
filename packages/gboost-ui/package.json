        "types": "./dist/UserManagement/UserManagement.d.ts",
        "import": "./dist/UserManagement/UserManagement.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
