    "@mantine/hooks": "^3.6.5",
    "@radix-ui/colors": "^0.1.8",
    "@stitches/react": "^1.2.6",
    "aws-amplify": "^4.3.14",
    "gboost-common": "workspace:^0.2.1",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.2.1"
