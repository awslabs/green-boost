  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./file-upload": "./src/FileUpload/FileUpload.ts",
    "./smart-fields": "./src/SmartFields/smart-fields.ts",
    "./user-management": "./src/UserManagement/UserManagement.ts"
