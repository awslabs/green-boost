{
  "name": "gboost-ui",
  "version": "0.30.1",
  "description": "Green Boost UI Library of React Components",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./OldQueryTable": "./src/OldQueryTable/QueryTable.ts",
    "./UserManagement": "./src/UserManagement/UserManagement.ts",
    "./FileUpload": "./src/FileUpload/FileUpload.ts"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.js",
    "module": "./lib/index.js",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js"
      },
      "./OldQueryTable": {
        "types": "./lib/OldQueryTable/QueryTable.d.ts",
        "import": "./lib/OldQueryTable/QueryTable.js"
      },
      "./UserManagement": {
        "types": "./lib/UserManagement/UserManagement.d.ts",
        "import": "./lib/UserManagement/UserManagement.js"
      },
      "./FileUpload": {
        "types": "./lib/FileUpload/FileUpload.d.ts",
        "import": "./lib/FileUpload/FileUpload.js"
      }
    },
    "types": "./lib/index.d.ts"
  },
  "files": [
    "lib/**/*"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^4.3.6",
    "@hookform/resolvers": "^2.9.10",
    "@mantine/hooks": "^3.6.14",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-dialog": "^0.1.7",
    "@radix-ui/react-portal": "^0.1.4",
    "@radix-ui/react-tooltip": "^0.1.7",
    "@stitches/react": "^1.2.8",
    "gboost-common": "workspace:^0.8.0",
    "graphql": "^15.8.0",
    "graphql-tag": "^2.12.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.43.0",
    "react-icons": "^4.7.1",
    "react-transition-group": "^4.4.5",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.262.0",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.18",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "aws-amplify": "^5.0.12",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.3",
    "react-router-dom": "^6.8.0",
    "rimraf": "^3.0.2",
    "typescript": "~4.7.4",
    "vite": "^2.9.15",
    "vitest": "^0.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/green-boost.git",
    "directory": "packages/gboost-ui"
  },
  "keywords": [
    "aws",
    "serverless",
    "react",
    "vite",
    "green",
    "boost",
    "gboost"
  ],
  "author": "AWS Professional Services",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/green-boost/issues"
  },
  "homepage": "https://awslabs.github.io/green-boost"
}
