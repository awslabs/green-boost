{
  "name": "gboost-ui",
  "version": "0.36.0",
  "description": "Green Boost UI Library of React Components",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      }
    },
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsx scripts/build.ts",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "gboost-common": "workspace:^"
  },
  "peerDependencies": {
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.3.0",
    "next": "^14.2.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/green-boost.git",
    "directory": "packages/gboost-ui"
  },
  "keywords": [
    "aws",
    "cloud",
    "native",
    "react",
    "next",
    "green",
    "boost",
    "gboost"
  ],
  "author": "AWS Professional Services",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/green-boost/issues"
  },
  "homepage": "https://awslabs.github.io/green-boost"
}
