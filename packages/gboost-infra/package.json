{
  "name": "gboost-infra",
  "version": "0.13.1",
  "description": "Green Boost Infrastructure Library of CDK Constructs",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./file-upload": "./src/file-upload/index.ts",
    "./user-management": "./src/user-management/index.ts"
  },
  "types": "./src/index.ts",
  "publishConfig": {
    "main": "./lib/index.js",
    "module": "./lib/index.js",
    "exports": {
      ".": {
        "types": "./lib/index.d.ts",
        "import": "./lib/index.js"
      },
      "./file-upload": {
        "types": "./lib/file-upload/index.d.ts",
        "import": "./lib/file-upload/index.js"
      },
      "./user-management": {
        "types": "./lib/user-management-upload/index.d.ts",
        "import": "./lib/user-management-upload/index.js"
      }
    },
    "types": "./lib/index.d.ts"
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "ts-node scripts/build.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-cdk/aws-appsync-alpha": "2.54.0-alpha.0",
    "@aws-sdk/client-cloudfront": "^3.303.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.303.0",
    "adm-zip": "^0.5.10",
    "aws-cdk-lib": "^2.72.1",
    "cdk-nag": "^2.25.2",
    "constructs": "^10.1.300",
    "gboost-common": "workspace:^0.10.0",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@aws-lambda-powertools/logger": "^1.7.0",
    "@aws-sdk/client-s3": "^3.304.0",
    "@aws-sdk/s3-request-presigner": "^3.304.0",
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/adm-zip": "^0.5.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "esbuild": "^0.17.15",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-define-config": "^1.17.0",
    "eslint-plugin-prettier": "^4.2.1",
    "joi": "^17.9.1",
    "prettier": "^2.8.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/awslabs/green-boost.git",
    "directory": "packages/gboost-infra"
  },
  "keywords": [
    "aws",
    "serverless",
    "cdk",
    "greenboost",
    "green",
    "boost",
    "gboost",
    "constructs"
  ],
  "author": "AWS Professional Services",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/awslabs/green-boost/issues"
  },
  "homepage": "https://awslabs.github.io/green-boost"
}
