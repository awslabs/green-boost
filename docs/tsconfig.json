{
  "extends": ["@tsconfig/strictest", "@tsconfig/next"],
  "compilerOptions": {
    "exactOptionalPropertyTypes": false,
    "paths": {
      "@/*": ["./src/*"],
    },
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
  ]
}
